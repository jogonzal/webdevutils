{
  "name": "react-webpack-typescript-starter",
  "version": "0.1.0",
  "author": "",
  "license": "",
  "sideEffects": [
    "*.css",
    "*.less",
    "*.scss"
  ],
  "scripts": {
    "build:dev": "npm run clean-dist && webpack -p --config=configs/webpack/dev.js --progress",
    "build:prod": "npm run clean-dist && webpack -p --config=configs/webpack/prod.js --progress",
    "build": "npm run build:prod",
    "clean-dist": "rimraf dist",
    "lint": "eslint ./src/**/*.{ts,tsx} ./tests/**/*.{ts,tsx}",
    "lint:fix": "eslint --fix ./src/**/*.{ts,tsx} ./tests/**/*.{ts,tsx}",
    "start": "npm run start:dev",
    "start:dev": "webpack-dev-server --config=configs/webpack/dev.js --progress",
    "start:prod": "webpack-dev-server --config=configs/webpack/prod.js --progress",
    "test": "jest"
  },
  "devDependencies": {
    "@types/debounce": "^1.2.0",
    "@types/faker": "^5.1.0",
    "@types/jest": "^25.2.1",
    "@types/papaparse": "^5.0.3",
    "@types/react": "^16.3.16",
    "@types/react-dom": "^16.0.6",
    "@types/react-sound": "^1.2.2",
    "@types/shortid": "^0.0.29",
    "@types/simple-peer": "^9.6.0",
    "@typescript-eslint/eslint-plugin": "^3.2.0",
    "@typescript-eslint/eslint-plugin-tslint": "^3.2.0",
    "@typescript-eslint/parser": "^3.2.0",
    "circular-dependency-plugin": "^5.2.0",
    "css-loader": "^3.5.2",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-compat": "3.6.0",
    "eslint-plugin-import": "2.20.2",
    "eslint-plugin-jsdoc": "25.4.2",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-no-null": "1.0.2",
    "eslint-plugin-react": "7.20.0",
    "eslint-plugin-react-hooks": "4.0.2",
    "file-loader": "^6.0.0",
    "fork-ts-checker-webpack-plugin": "^4.1.4",
    "html-webpack-plugin": "^4.2.0",
    "jest": "^25.3.0",
    "jest-junit": "^10.0.0",
    "mini-css-extract-plugin": "^0.9.0",
    "papaparse": "^5.2.0",
    "postcss-loader": "^3.0.0",
    "prettier": "2.0.5",
    "rimraf": "^2.6.2",
    "script-ext-html-webpack-plugin": "^2.1.4",
    "style-loader": "^1.1.4",
    "terser-webpack-plugin": "^2.3.5",
    "ts-jest": "^25.4.0",
    "ts-loader": "^7.0.0",
    "typescript": "^3.9.5",
    "webpack": "^4.42.1",
    "webpack-bundle-analyzer": "^3.7.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3",
    "webpack-merge": "^4.2.2"
  },
  "dependencies": {
    "@aspnet/signalr": "^1.1.4",
    "@microsoft/applicationinsights-web": "^2.3.1",
    "@types/match-sorter": "^2.2.0",
    "@types/react-router-dom": "^4.3.1",
    "debounce": "^1.2.0",
    "dexie": "^3.0.1",
    "es6-promise-pool": "^2.5.0",
    "faker": "^5.1.0",
    "match-sorter": "^2.3.0",
    "office-ui-fabric-react": "^7.107.1",
    "react": "^16.8.0",
    "react-diff-viewer": "^2.0.5",
    "react-dom": "^16.8.0",
    "react-google-charts": "^3.0.15",
    "react-router-dom": "^5.1.2",
    "react-sound": "^1.2.0",
    "shortid": "^2.2.15",
    "simple-peer": "^9.7.2",
    "sweetalert2": "^9.10.9",
    "sweetalert2-react-content": "^3.0.1"
  },
  "postcss": {},
  "jest": {
    "testURL": "https://localhost",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/(?!(office-ui-fabric-react))"
    ],
    "testRegex": "(/__tests__/.*|(\\.|/)(tests|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "moduleNameMapper": {
      "\\.css|.css?raw$": "identity-obj-proxy",
      "\\.less": "identity-obj-proxy",
      "\\.(jpg|ico|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "identity-obj-proxy",
      "office-ui-fabric-react/lib/(.*)$": "office-ui-fabric-react/lib-commonjs/$1"
    },
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "coverageReporters": [
      "cobertura",
      "json",
      "html"
    ],
    "reporters": [
      "default",
      "jest-junit"
    ]
  }
}
