{
  "name": "posadation",
  "version": "0.1.0",
  "author": "",
  "license": "",
  "sideEffects": [
    "*.css",
    "*.less",
    "*.scss"
  ],
  "scripts": {
    "build:dev": "npm run clean-dist && webpack --config=configs/webpack/dev.js --progress",
    "build:prod": "npm run clean-dist && webpack --config=configs/webpack/prod.js --progress",
    "build": "npm run build:prod",
    "clean-dist": "rimraf dist",
    "lint": "eslint ./src/**/*.{ts,tsx} ./tests/**/*.{ts,tsx}",
    "lint:fix": "eslint --fix ./src/**/*.{ts,tsx} ./tests/**/*.{ts,tsx}",
    "start": "npm run start:dev",
    "start:dev": "webpack serve --config=configs/webpack/dev.js --progress",
    "start:prod": "webpack serve --config=configs/webpack/prod.js --progress",
    "test": "jest",
    "start:ngrok": "ngrok http 5000"
  },
  "devDependencies": {
    "@types/jest": "^26.0.15",
    "@types/papaparse": "^5.0.3",
    "@types/qrcode": "^1.3.5",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-router-dom": "^5.1.6",
    "@types/react-sound": "^1.2.2",
    "@types/shortid": "^0.0.29",
    "@typescript-eslint/eslint-plugin": "^4.8.1",
    "@typescript-eslint/eslint-plugin-tslint": "^4.8.1",
    "@typescript-eslint/parser": "^4.8.1",
    "circular-dependency-plugin": "^5.2.2",
    "css-loader": "^5.0.1",
    "eslint": "7.14.0",
    "eslint-config-prettier": "6.15.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-compat": "3.8.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsdoc": "30.7.8",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-no-null": "1.0.2",
    "eslint-plugin-react": "7.21.5",
    "eslint-plugin-react-hooks": "4.2.0",
    "eslint-plugin-simple-import-sort": "^6.0.1",
    "file-loader": "^6.2.0",
    "fork-ts-checker-webpack-plugin": "^6.0.3",
    "html-webpack-plugin": "^4.5.0",
    "jest": "^26.6.3",
    "jest-junit": "^12.0.0",
    "mini-css-extract-plugin": "^0.9.0",
    "ngrok": "^3.3.0",
    "papaparse": "^5.2.0",
    "prettier": "2.2.0",
    "rimraf": "^3.0.2",
    "script-ext-html-webpack-plugin": "^2.1.4",
    "style-loader": "^2.0.0",
    "terser-webpack-plugin": "^5.0.3",
    "ts-jest": "^26.4.4",
    "ts-loader": "^8.0.11",
    "typescript": "^4.1.2",
    "webpack": "^5.6.0",
    "webpack-bundle-analyzer": "^4.1.0",
    "webpack-cli": "^4.2.0",
    "webpack-dev-server": "^3.10.3",
    "webpack-merge": "^5.4.0"
  },
  "dependencies": {
    "@aspnet/signalr": "^1.1.4",
    "@fluentui/react": "^7.152.1",
    "@microsoft/applicationinsights-web": "^2.3.1",
    "chart.js": "^2.9.3",
    "dexie": "^3.0.1",
    "es6-promise-pool": "^2.5.0",
    "jwt-decode": "^3.1.2",
    "qrcode": "^1.4.4",
    "react": "^17.0.1",
    "react-chartjs-2": "^2.10.0",
    "react-csv-reader": "^3.1.1",
    "react-diff-viewer": "^3.1.1",
    "react-dom": "^17.0.1",
    "react-google-charts": "^3.0.15",
    "react-router-dom": "^5.1.2",
    "react-sound": "^1.2.0",
    "shortid": "^2.2.16",
    "sweetalert2": "^10.10.1",
    "sweetalert2-react-content": "^3.0.1"
  },
  "postcss": {},
  "jest": {
    "testURL": "https://localhost",
    "preset": "ts-jest",
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/(?!(@fluentui/react))"
    ],
    "testRegex": "(/__tests__/.*|(\\.|/)(tests|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "moduleNameMapper": {
      "\\.css|.css?raw$": "identity-obj-proxy",
      "\\.less": "identity-obj-proxy",
      "\\.(jpg|ico|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga|wac)$": "identity-obj-proxy",
      "@fluentui/react/lib/(.*)$": "@fluentui/react/lib-commonjs/$1"
    },
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "coverageReporters": [
      "cobertura",
      "json",
      "html"
    ],
    "reporters": [
      "default",
      "jest-junit"
    ]
  }
}
