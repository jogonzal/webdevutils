{
  "name": "webdevutils",
  "version": "0.1.0",
  "author": "Jorge",
  "license": "UNLICENSED",
  "sideEffects": [
    "*.css",
    "*.less",
    "*.scss"
  ],
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "clean-dist": "rimraf dist",
    "lint": "next lint",
    "test": "jest",
    "typescript-build": "tsc",
    "detect-dead-code:noinclude": "ts-prune -p tsconfig.ts-pruneNoInclude.json > pruneoutput.txt",
    "detect-dead-code:include": "ts-prune -p tsconfig.ts-prune.json > pruneoutputNoInclude.txt ",
    "detect-dead-code": "npm run detect-dead-code:include && npm run detect-dead-code:noinclude && ts-node --project scripts/tsconfig.json scripts/printPruneInfo.ts",
    "fail-if-dead-code": "npm run detect-dead-code:include && npm run detect-dead-code:noinclude && ts-node --project scripts/tsconfig.json scripts/failIfPruningNeeded.ts",
    "start:ngrok": "ngrok http 5000",
    "pretty": "prettier --write \"./**/*.{js,jsx,mjs,cjs,ts,tsx,json}\""
  },
  "devDependencies": {
    "@types/jest": "^26.0.23",
    "@types/papaparse": "^5.2.5",
    "@types/qrcode": "^1.4.0",
    "@types/react": "^18.2.28",
    "@types/react-dom": "^18.2.13",
    "@types/react-sound": "^1.2.2",
    "@types/shortid": "^0.0.29",
    "circular-dependency-plugin": "^5.2.2",
    "eslint": "^8.51.0",
    "eslint-config-next": "^13.5.4",
    "jest": "^27.0.6",
    "jest-junit": "^12.2.0",
    "ngrok": "^4.0.1",
    "papaparse": "^5.3.1",
    "prettier": "2.2.0",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.0.3",
    "ts-node": "^10.0.0",
    "ts-prune": "^0.9.2",
    "typescript": "5.2.2"
  },
  "dependencies": {
    "@aspnet/signalr": "^1.1.4",
    "@fluentui/react": "^8.22.0",
    "chart.js": "^2.9.3",
    "content-security-policy-parser": "^0.4.1",
    "dexie": "^3.0.1",
    "es6-promise-pool": "^2.5.0",
    "fast-xml-parser": "^3.20.0",
    "jwt-decode": "^3.1.2",
    "next": "^13.5.4",
    "qrcode": "^1.4.4",
    "react": "^18.2.0",
    "react-chartjs-2": "^3.0.3",
    "react-csv-reader": "^3.1.1",
    "react-diff-viewer": "^3.1.1",
    "react-dom": "^18.2.0",
    "react-google-charts": "^3.0.15",
    "react-sound": "^1.2.0",
    "shortid": "^2.2.16",
    "sweetalert2": "^11.0.18",
    "sweetalert2-react-content": "^4.1.0",
    "ts-exif-parser": "^0.2.2"
  },
  "postcss": {},
  "jest": {
    "testURL": "https://localhost",
    "preset": "ts-jest",
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/(?!(@fluentui/react))"
    ],
    "testRegex": "(/__tests__/.*|(\\.|/)(tests|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "moduleNameMapper": {
      "\\.css|.css?raw$": "identity-obj-proxy",
      "\\.less": "identity-obj-proxy",
      "\\.(jpg|ico|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga|wac)$": "identity-obj-proxy",
      "@fluentui/react/lib/(.*)$": "@fluentui/react/lib-commonjs/$1"
    },
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "coverageReporters": [
      "cobertura",
      "json",
      "html"
    ],
    "reporters": [
      "default",
      "jest-junit"
    ]
  }
}
